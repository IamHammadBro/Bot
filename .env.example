# Environment Variables for Spotify Premium Bot
# Copy this file to .env and fill in your actual values

# Bot Configuration
DEBUG=false
PORT=5000

# GitHub Developer Pack API Keys
# Get these from GitHub Student Developer Pack
BRIGHT_DATA_PROXY_URL=your_bright_data_proxy_url_here
BRIGHT_DATA_USERNAME=your_bright_data_username_here
BRIGHT_DATA_PASSWORD=your_bright_data_password_here
FINGERPRINTJS_API_KEY=your_fingerprintjs_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
SENTRY_DSN=your_sentry_dsn_here

# Optional: Spotify Web API (for enhanced intelligence)
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Deployment Configuration
RENDER_EXTERNAL_HOSTNAME=your-app-name.onrender.com
